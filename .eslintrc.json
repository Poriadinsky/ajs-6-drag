{
    "extends": "airbnb-base",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "rules": {
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ],
      "no-plusplus": 0,
      "no-useless-constructor": 0,
      "no-empty-function": 0,
      "no-param-reassign": 0,
      "no-restricted-globals": 0,
      "no-use-before-define": 0,
      "no-mixed-operators": 0,
      "class-methods-use-this": 0,
      "import/no-named-as-default-member": 0,
      "import/no-named-as-default": 0
    }
  }